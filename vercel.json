{
  "version": 2,
  "builds": [
    {
      "src": "./bin/www",
      "use": "@vercel/node",
      "config": {
        "includeFiles": ["**"]
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/bin/www"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "functions": {
    "bin/www": {
      "maxDuration": 30
    }
  }
}
