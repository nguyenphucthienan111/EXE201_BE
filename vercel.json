{
  "version": 2,
  "builds": [
    {
      "src": "./bin/www",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/bin/www"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "functions": {
    "bin/www": {
      "maxDuration": 30
    }
  },
  "installCommand": "npm install --legacy-peer-deps"
}
