{
  "version": 2,
  "builds": [
    {
      "src": "./bin/www",
      "use": "@vercel/node",
      "config": {
        "maxLambdaSize": "50mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/bin/www"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "installCommand": "npm install --legacy-peer-deps"
}
